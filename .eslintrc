{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ],
    "no-underscore-dangle": false
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
